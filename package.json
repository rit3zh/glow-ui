{
  "name": "glow-ui",
  "version": "1.13.2",
  "main": "expo-router/entry",
  "bin": {
    "glow-ui": "./cli/dist/index.js"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint --config .eslintrc.js 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --config .eslintrc.js 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "build": "tsc",
    "version": "standard-version",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "docs": "typedoc",
    "docs:deploy": "gh-pages -d docs",
    "release": "npm run build && npm run version && npm run docs && npm run docs:deploy && git push --follow-tags origin main"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": false
    },
    "commitAll": true
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.2",
    "@lodev09/react-native-true-sheet": "^2.0.5",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/elements": "^2.4.2",
    "@react-navigation/material-top-tabs": "^7.2.13",
    "@react-navigation/native": "^7.0.3",
    "@react-navigation/native-stack": "^7.3.12",
    "@react-navigation/stack": "^7.0.4",
    "expo": "~52.0.46",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-video": "~2.0.6",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "~0.77.1",
    "react-native-animated-rolling-numbers": "^2.0.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.60",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-pager-view": "^6.7.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-webview": "~13.13.1",
    "rn-fade-wrapper": "^0.1.9",
    "swiftui-native": "^1.0.4",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "release-it": "^19.0.1",
    "standard-version": "^9.5.0",
    "typedoc": "^0.28.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.31.1"
  },
  "private": true
